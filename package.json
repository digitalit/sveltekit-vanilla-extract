{
  "name": "sveltekit-vanilla-extract",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --port 3000",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "husky install",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "svelte-check": "svelte-check --fail-on-hints"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^15.0.0",
    "@fontsource/source-sans-pro": "^4.5.1",
    "@fontsource/source-serif-pro": "^4.5.0",
    "@sveltejs/adapter-static": "^1.0.0-next.22",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/vite-plugin": "3.1.2",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.4",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "sharp": "0.28.2",
    "svelte": "^3.44.3",
    "svelte-check": "^2.2.11",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vanilla-lazyload": "^17.5.0",
    "vite": "^2.7.6",
    "vite-imagetools": "^4.0.3"
  },
  "type": "module"
}
